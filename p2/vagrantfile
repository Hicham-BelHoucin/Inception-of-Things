Vagrant.configure("2") do |config|
    config.vm.define "obeajS" do |obeajS|
        obeajS.vm.box = "ubuntu/focal64"
        obeajS.vm.hostname = "obeajS"
        obeajS.vm.network "private_network", ip: "192.168.56.110"
        obeajS.vm.synced_folder "./shared", "/vagrant/shared", create: true
        system("cp -r config/ shared/")
        obeajS.vm.provision "shell", path: "./scripts/server.sh"
        obeajS.vm.provider "virtualbox" do |vb|
            vb.memory = "1024"
            vb.cpus = "1"
        end
    end
end